<html	xmlns="http://www.w3.org/1999/xhtml"
		xmlns:h="http://java.sun.com/jsf/html"
     	xmlns:f="http://java.sun.com/jsf/core"
      	xmlns:p="http://primefaces.org/ui">
	<h:head>
		<h:outputLabel value="Add a New Book!"/>
	</h:head>
	<h:body>
		<p:panel header="Book Details" style="width :700;">
			<p:panelGrid columns="2" id="bookEditPanelGrid">
				<p:outputLabel value ="Title: "/> <p:inputText id="titleID" value="#{bookMB.book.title}"/>
				<p:outputLabel value ="Max Checkout Length: "/> <p:inputText id="maxcheckoutlengthID" value="#{bookMB.book.maxcheckoutlength}"/>
				<p:outputLabel value ="ISBN: "/> <p:inputText id="ISBN" value="#{bookMB.book.iSBN}"/>
				<p:button value="save" outcome=""/>
			</p:panelGrid>
		</p:panel>
	</h:body>
</html>