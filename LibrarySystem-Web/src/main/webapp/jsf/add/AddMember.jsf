<html	xmlns="http://www.w3.org/1999/xhtml"
		xmlns:h="http://java.sun.com/jsf/html"
     	xmlns:f="http://java.sun.com/jsf/core"
      	xmlns:p="http://primefaces.org/ui">
	<h:head>
		<h:outputLabel value="Add a New Member!"/>
	</h:head>
	<h:body>
		<p:dataTable>
			<p:row>
				<p:outputLabel value ="First Name: "/> <p:inputText id="firstNameID" value="#{memberMB.member.firstname}"/>
			</p:row>
			<p:row>
				<p:outputLabel value ="Last Name: "/> <p:inputText id="lastNameID" value="#{memberMB.member.lastname}"/>
			</p:row>
			<p:row>
				<p:outputLabel value ="Phone: "/> <p:inputText id="phoneID" value="#{memberMB.member.phone}"/>
			</p:row>
			<p:row>
				<p:outputLabel value ="Street: "/> <p:inputText id="streetID" value="#{memberMB.address.street}"/>
			</p:row>
			<p:row>
				<p:outputLabel value ="City: "/> <p:inputText id="cityID" value="#{memberMB.address.city}"/>
			</p:row>
			<p:row>
				<p:outputLabel value ="State: "/> <p:inputText id="stateID" value="#{memberMB.address.state}"/>
			</p:row>
			<p:row>
				<p:outputLabel value ="ZipCode: "/> <p:inputText id="zipcodeID" value="#{memberMB.address.zipcode}"/>
			</p:row>
			<p:row>
				<p:outputLabel value ="Member Number: "/> <p:inputText id="memberNumID" value="#{memberMB.member.member_no}"/>
			</p:row>
			<p:row>
				<p:button value="save" outcome=""/>
			</p:row>
		</p:dataTable>
	</h:body>
</html>